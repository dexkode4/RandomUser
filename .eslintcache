ader handleChange={handleChange} />\n          <UserDetails />\n        </PageLayout>\n      </Route>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/macbook/Desktop/Dexkode/ReactProject/RandUserGen/src/components/PageLayout/index.tsx",[],["93","94"],"/Users/macbook/Desktop/Dexkode/ReactProject/RandUserGen/src/index.tsx",[],["95","96"],"/Users/macbook/Desktop/Dexkode/ReactProject/RandUserGen/src/components/SideNavContainer/index.tsx",["97"],"import React, { useState } from 'react';\nimport styles from './SideNav.module.scss';\nimport { FaUsers, FaMale, FaFemale } from 'react-icons/fa';\nimport Button from '../CustomButton';\nimport SearchField from '../SearchField';\n\nconst buttonsDetails = [\n    {\n        icon: <FaUsers />,\n        title: \"All Users\"\n    },\n    {\n        icon: <FaMale />,\n        title: \"Male Users\"\n    },\n    {\n        icon: <FaFemale />,\n        title: \"Female Users\"\n    },\n]\n\n\nfunction SideNav() {\n    // const [state, setState] = useState(\"All Users\");\n\n    return (\n        <div className={styles.sideNav}>\n            <header className={styles.sideNavGreetings}>Hello, <span className={styles.sideNavName}>Emerald</span></header>\n            <p className={styles.sideNavWelcomeMsg}>Welcome to your dashboard, kindly sort through the user base</p>\n            <SearchField placeholder=\"Find a user\" size=\"big\" />\n            <header className={styles.showUser}>Show Users</header>\n            <div className={styles.buttonGroup}>\n                {\n                    buttonsDetails.map((item, index) => (<Button key={index} title={item.title} icon={item.icon}/>))\n                }\n            </div>\n        </div>\n    )\n}\n\nexport default SideNav\n",["98","99"],"/Users/macbook/Desktop/Dexkode/ReactProject/RandUserGen/src/components/MainContainer/index.tsx",[],["100","101"],"/Users/macbook/Desktop/Dexkode/ReactProject/RandUserGen/src/components/UserList/index.tsx",["102","103","104"],"import React, { 